    <TextView
        android:id="@+id/text_book_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="17dp"
        android:text="ID-ul cartii"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/text_label_book_quote"
        app:layout_constraintTop_toBottomOf="@id/text_label_id_user_quote" />

    <TextView
        android:id="@+id/text_label_book_title_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Titlul cartii:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="8dp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_label_book_quote"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text_book_title_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Titlu cartea extrasa"
        android:textSize="16sp"
        android:layout_margin="12dp"
        app:layout_constraintTop_toBottomOf="@id/text_label_book_title_quote"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text_label_book_author_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Autorul cartii:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_book_title_quote"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text_book_author_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Autor Cartea extrasa"
        android:textSize="16sp"
        android:layout_margin="12dp"
        app:layout_constraintTop_toBottomOf="@id/text_label_book_author_quote"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>

