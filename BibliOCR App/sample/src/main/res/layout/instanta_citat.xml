<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.canhub.cropper.sample.bazadedate.PaginaCitatActivity">

    <TextView
        android:id="@+id/text_titlu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citatul"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_margin="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/text_view_quote"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Textul Citatului"
        android:textColor="@android:color/darker_gray"
        android:textSize="16sp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_titlu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/text_label_id_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID Citat:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@id/text_view_quote"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text_view_id_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID Citat"
        android:textSize="16sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@id/text_view_quote"
        app:layout_constraintStart_toEndOf="@id/text_label_id_quote"/>

    <TextView
        android:id="@+id/text_label_id_user_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citatul a fost intrudus de:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_margin="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_view_id_quote"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/text_view_id_user_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nume utilizator"
        android:textSize="16sp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_view_id_quote"
        app:layout_constraintStart_toEndOf="@+id/text_label_id_user_quote"/>

    <TextView
        android:id="@+id/text_label_book_quote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citat extras din cartea cu idul:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toBottomOf="@id/text_label_id_user_quote"
        app:layout_constraintStart_toStartOf="parent"/>


