<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.canhub.cropper.sample.LoginAdminActivity">

    <EditText
        android:id="@+id/edit_text_adminname"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Nume Admin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="16dp"
        android:inputType="" />

    <EditText
        android:id="@+id/edit_text_adminpassword"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="Parola"
        android:inputType="textPassword"
        app:layout_constraintTop_toBottomOf="@id/edit_text_adminname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="16dp"/>

    <Button
        android:id="@+id/btn_login_admin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Logare"
        app:layout_constraintTop_toBottomOf="@id/edit_text_adminpassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
